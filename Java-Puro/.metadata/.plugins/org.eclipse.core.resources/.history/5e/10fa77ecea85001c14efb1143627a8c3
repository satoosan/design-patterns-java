package one.digitalinnovation.gof;

/*
 * Singleton "apressado".
 * 
 * @author satoosan
 * 
 */

public class SingletonEager {
	
	private static SingletonEager instancia;
		
	public SingletonEager() {
		super();
	}
	
	public static SingletonEager getInstancia() {
		if(instancia == null) {
			instancia = new SingletonEager();
		}
		return instancia;
	}
	
}
